<mvc:View
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc">
   <List
      headerText="{i18n>invoiceListTitle}"
      class="sapUiResponsiveMargin"
      width="auto"
      items="{invoice&gt;/InvoiceSet}">
      <items>
         <ObjectListItem title="{invoice&gt;Productname}"></ObjectListItem>
         <ObjectListItem
            title="{invoice&gt;Quantity} x {invoice&gt;Productname}"/>
      </items>
   </List></mvc:View>